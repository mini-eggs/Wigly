{
  "name": "wigly",
  "version": "0.5.1",
  "main": "dist/wigly.es5.js",
  "module": "src/main.js",
  "scripts": {
    "build": "rm -rf dist && webpack --mode production && gzip -f -k dist/*",
    "test": "ava",
    "size": "wc -c dist/*"
  },
  "dependencies": {
    "superfine": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "ava": "1.0.0-rc.2",
    "babel-loader": "^8.0.4",
    "browser-env": "^3.2.5",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "plugins": [
          [
            "@babel/plugin-transform-react-jsx",
            {
              "pragma": "h"
            }
          ]
        ]
      }
    }
  }
