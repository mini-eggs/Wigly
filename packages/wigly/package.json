{
  "name": "wigly",
  "main": "dist/es5.js",
  "version": "0.1.21",
  "scripts": {
    "start": "npm run clean && npm run es5 && npm run es6 && npm run gzip",
    "clean": "rm -rf dist",
    "es5": "node_modules/google-closure-compiler-linux/compiler --isolation_mode IIFE --module_resolution WEBPACK --language_in=ECMASCRIPT_NEXT --language_out=ECMASCRIPT5 --compilation_level ADVANCED_OPTIMIZATIONS --js_output_file=dist/es5.js 'src/**.js'",
    "es6": "node_modules/google-closure-compiler-linux/compiler --isolation_mode IIFE --module_resolution WEBPACK --language_in=ECMASCRIPT_NEXT --language_out=ECMASCRIPT6 --compilation_level ADVANCED_OPTIMIZATIONS --js_output_file=dist/es6.js 'src/**.js'",
    "gzip": "gzip dist/* -k -f",
    "test": "ava",
    "size": "wc -c dist/es6.js.gz | cut -d' ' -f1"
  },
  "peerDependencies": {
    "wigly-component": "^0.1.x",
    "wigly-dom": "^0.1.x",
    "wigly-jsx": "^0.1.x"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-rc.3",
    "ava": "1.0.0-beta.7",
    "browser-env": "^3.2.5",
    "google-closure-compiler-linux": "^20180805.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "wigly-component": "^0.1.21",
    "wigly-dom": "^0.1.21",
    "wigly-jsx": "^0.1.21"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    },
    "compileEnhancements": false,
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [
    "ui",
    "component",
    "reactive",
    "frontend",
    "jsx",
    "vdom",
    "virtual-dom",
    "declarative",
    "framework"
  ],
  "homepage": "https://github.com/mini-eggs/wigly",
  "bugs": "https://github.com/mini-eggs/wigly/issues",
  "license": "MIT",
  "repository": "https://github.com/mini-eggs/wigly"
}
